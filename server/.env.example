# Database Configuration with Connection Pooling
# Add these parameters to your DATABASE_URL to optimize connections:
# ?connection_limit=10&pool_timeout=20&connect_timeout=60
DATABASE_URL="postgresql://username:password@localhost:5432/database?connection_limit=10&pool_timeout=20&connect_timeout=60"

# Server Configuration
PORT=3000
NODE_ENV=development
BASE_URL=http://localhost:3000

# Authentication
JWT_SECRET=your-jwt-secret-key

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_BUCKET_NAME=your-bucket-name

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# External APIs
REPLICATE_UPLOAD_IMAGE_URL=https://api.replicate.com/v1/upload
REPLICATE_UPLOAD_IMAGE_TOKEN=your-replicate-token
