# Default model used for the Flux-style generation (keep as the model/version you were using before)
REPLICATE_MODEL_VERSION=owner/flux-konect:version

# Optional: explicit replicate model id for Nano Banana (defaults to google/nano-banana)
NANOBANANA_REPLICATE_MODEL=google/nano-banana

# Database Configuration
DATABASE_URL=your_database_url

